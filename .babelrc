{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {}
      }
    ],
    "transform-promise-to-bluebird",
    "@babel/plugin-proposal-class-properties"
  ]
}
